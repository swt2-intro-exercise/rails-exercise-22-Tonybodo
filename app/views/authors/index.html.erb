<h1>Authors</h1>
<table>
    <tr>
        <th>Name</th>
        <th>Homepage</th>
    </tr>

    <% @authors.each do |author| %>
        <tr>
            <td><%= link_to author.name, author_path(author) %></td>
            <td><%= link_to author.homepage, author.homepage %></td>
        </tr>
    <% end %>
</table>

<br />
<td><%= link_to "New", new_author_path %></td>
